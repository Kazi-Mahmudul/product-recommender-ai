{
  "environmentVariables": {
    "NODE_ENV": "production",
    "LOG_LEVEL": "info",
    "PORT": "3001",
    "ENABLE_METRICS": "true",
    "ENABLE_HEALTH_CHECKS": "true",
    "HEALTH_CHECK_INTERVAL": "30000",
    "QUOTA_CHECK_INTERVAL": "60000",
    "FALLBACK_ENABLED": "true",
    "FALLBACK_TIMEOUT": "5000"
  },
  "environmentProfiles": {
    "development": {
      "LOG_LEVEL": "debug",
      "ENABLE_METRICS": "true",
      "ENABLE_HEALTH_CHECKS": "true"
    },
    "test": {
      "LOG_LEVEL": "error",
      "ENABLE_METRICS": "false",
      "ENABLE_HEALTH_CHECKS": "false"
    },
    "production": {
      "LOG_LEVEL": "info",
      "ENABLE_METRICS": "true",
      "ENABLE_HEALTH_CHECKS": "true"
    }
  }
}