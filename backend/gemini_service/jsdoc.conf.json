{
  "source": {
    "include": [
      "./",
      "./utils/",
      "./managers/",
      "./parsers/",
      "./monitoring/",
      "./quota/",
      "./config/"
    ],
    "exclude": [
      "node_modules/",
      "test/",
      "coverage/",
      "docs/",
      "scripts/"
    ],
    "includePattern": "\\.(js)$",
    "excludePattern": "\\.test\\.js$"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "metadata": {
    "title": "AI Quota Management Service Documentation",
    "version": "2.0.0"
  }
}