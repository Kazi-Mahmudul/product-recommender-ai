{
  "name": "ai-quota-management-service",
  "version": "2.0.0",
  "description": "AI-powered phone recommendation service with intelligent quota management, health monitoring, and multi-provider fallback support",
  "main": "index.js",
  "keywords": [
    "ai",
    "quota-management",
    "phone-recommendations",
    "gemini",
    "fallback",
    "health-monitoring",
    "multi-provider"
  ],
  "author": "ePick Development Team",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "node test/integration/run-integration-tests.js",
    "test:basic-integration": "node test/integration/run-basic-integration.js",
    "test:e2e": "jest test/integration/end-to-end.test.js --detectOpenHandles --forceExit",
    "test:load": "jest test/integration/load-test.test.js --detectOpenHandles --forceExit",
    "test:failover": "jest test/integration/provider-failover.test.js --detectOpenHandles --forceExit",
    "test:performance": "jest test/integration/fallback-performance.test.js --detectOpenHandles --forceExit",
    "validate-config": "node scripts/validate-config.js",
    "check-health": "node scripts/health-check.js",
    "export-metrics": "node scripts/export-metrics.js",
    "reset-quota": "node scripts/reset-quota.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "docs": "jsdoc -c jsdoc.conf.json",
    "prestart": "npm run validate-config",
    "postinstall": "node scripts/setup.js",
    "setup-monitoring": "node scripts/setup-monitoring.js",
    "start-monitoring": "node start-monitoring.js",
    "monitoring-dashboard": "node start-monitoring.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jsdoc": "^4.0.2",
    "@types/jest": "^29.5.8",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epick/ai-quota-management-service.git"
  },
  "bugs": {
    "url": "https://github.com/epick/ai-quota-management-service/issues"
  },
  "homepage": "https://github.com/epick/ai-quota-management-service#readme",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!scripts/**",
      "!docs/**",
      "!*.config.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "**/*.js",
      "config/**/*.json"
    ],
    "ignore": [
      "node_modules/**",
      "test/**",
      "coverage/**",
      "docs/**"
    ],
    "env": {
      "NODE_ENV": "development"
    }
  }
}